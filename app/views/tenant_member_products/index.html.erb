  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %> of <%= user_full_name(@user) %></h1>
</div>
<%= render 'products/list', obj: @products %>
<%= link_to t('.back', :default => t("helpers.links.back")),
            root_path, :class => 'btn btn-default'  %>